<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Super Fast Food</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/Javascript/SuperFoodAppDependencies")
    @Scripts.Render("~/bundles/Javascript/SuperFoodAdministrationApp")
    @Scripts.Render("~/bundles/Javascript/SuperFoodHomeDashboardApp")
</head>
<body style="margin-top: 50px; margin-bottom: 50px">
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Super Fast Food</a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#home/dashboard">Top Deals <span class="sr-only">(current)</span></a></li>
                    <li><a href="#about">About</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Menu <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#home/dashboard?key=getbycategory&value=Breakfast">Breakfast</a></li>
                            <li><a href="#home/dashboard?key=getbycategory&value=Lunch">Lunch</a></li>
                            <li><a href="#home/dashboard?key=getbycategory&value=Dinner">Dinner</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#home/dashboard?key=getbytype&value=Pizza">Pizza</a></li>
                            <li><a href="#home/dashboard?key=getbytype&value=Sandwich">Sandwich</a></li>
                            <li><a href="#home/dashboard?key=getbytype&value=Salad">Salad</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#home/dashboard?key=getbytype&value=Drinks">Drinks</a></li>
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Administration <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#administration/product">Product</a></li>
                            <li><a href="#administration/producttypes">Product Type</a></li>
                            <li><a href="#administration/order">Order</a></li>
                        </ul>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Account <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">My Profile</a></li>
                            <li><a href="#">Payment Settings</a></li>
                            <li role="separator" class="divider"></li>
                            <li><a href="#">Log Out</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/salvattore.min.js"></script>
</body>
</html>
